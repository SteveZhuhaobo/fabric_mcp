# Development Environment Configuration
# Microsoft Fabric Lakehouse MCP Server

# Required: Microsoft Fabric Configuration
FABRIC_WORKSPACE_ID=dev-workspace-id
FABRIC_LAKEHOUSE_ID=dev-lakehouse-id
FABRIC_TENANT_ID=your-tenant-id

# Authentication Configuration
FABRIC_AUTH_METHOD=interactive
FABRIC_CLIENT_ID=your-dev-client-id
FABRIC_CLIENT_SECRET=your-dev-client-secret

# Development Logging Configuration
LOG_LEVEL=DEBUG
STRUCTURED_LOGGING=true

# Development Limits (smaller for testing)
MAX_QUERY_TIMEOUT=60
MAX_RESULT_ROWS=1000
ENABLE_WRITE_OPERATIONS=true

# Development Retry Configuration
RETRY_ATTEMPTS=2
RETRY_BACKOFF_FACTOR=1.5