# Microsoft Fabric Configuration
FABRIC_WORKSPACE_ID=your-workspace-id-here
FABRIC_LAKEHOUSE_ID=your-lakehouse-id-here
FABRIC_TENANT_ID=your-tenant-id-here

# Authentication Configuration
FABRIC_AUTH_METHOD=service_principal
FABRIC_CLIENT_ID=your-client-id-here
FABRIC_CLIENT_SECRET=your-client-secret-here

# Server Configuration
FABRIC_MAX_QUERY_TIMEOUT=300
FABRIC_MAX_RESULT_ROWS=10000
FABRIC_ENABLE_WRITE_OPERATIONS=true

# Logging Configuration
LOG_LEVEL=INFO

# API Configuration
FABRIC_BASE_URL=https://api.fabric.microsoft.com
FABRIC_RETRY_ATTEMPTS=3
FABRIC_RETRY_BACKOFF_FACTOR=1.0